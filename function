// math.js
function sum(a,b){
    console.log("sum "+ (a+b));
}
function sub(a,b){
    console.log("sub "+ (a-b));
}
function mul(a,b){
    console.log("mul " + (a*b));
}
module.exports={sum,sub,mul};
